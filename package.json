{
  "name": "typed-stack",
  "version": "0.9.1",
  "description": "A Javascript/Typescript implementation of the stack data-structure.",
  "main": "./dist/typed-stack",
  "type": "module",
  "scripts": {
    "test:coverage": "jest --coverage --coverageDirectory='coverage'",
    "test": "jest --verbose --silent",
    "clean": "shx rm -rf lib/*",
    "docs:html": "typedoc src/index.ts",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs/md src/index.ts",
    "build": "rollup -c rollup.config.js"
  },
  "exports": {
    ".": {
      "import": "./dist/typed-stack.mjs",
      "require": "./dist/typed-stack.js",
      "types": "./dist/typed-stack.d.ts",
      "source": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisitopherus/typed-stack.git"
  },
  "keywords": [
    "stack",
    "typescript",
    "generics",
    "simple",
    "browser",
    "node"
  ],
  "author": "chrisitopherus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisitopherus/typed-stack/issues"
  },
  "homepage": "https://github.com/chrisitopherus/typed-stack#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "jest": "^29.4.3",
    "rollup": "^3.17.3",
    "rollup-plugin-cjs-check": "^1.0.2",
    "rollup-plugin-dts": "^5.2.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.5"
  }
}
